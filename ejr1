// Ingresar 25 n√∫meros, calcular su promedio e imprimirlo


int cant = 25;
int sumt = 0;


for (int i = 0; i < cant; i++)
{

    Console.WriteLine("ingresar 25 valores numericos:");
    int nums = int.Parse(Console.ReadLine());
    sumt += nums;
}

int prom = sumt / cant;

Console.WriteLine("el promedio es igual a: " + prom);
